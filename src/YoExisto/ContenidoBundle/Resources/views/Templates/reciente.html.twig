{% extends "YoExistoContenidoBundle:Templates:dashboard.html.twig" %}

{% block menu %}
	{% include 'YoExistoContenidoBundle:Blocks:menu-user.html.twig' %}
{% endblock %}


{% block contenidointerno %}
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-4 txt-mayusculas">
				<a href="{{ path('yoexisto_donde') }}" class="btn btn-home btn-tipo-a">Generar Control</a>
			</div>
		</div>

		<div class="row espacio-a">
			<div class="col-md-8 txt-mayusculas">
				<h3 class="texto-naranja txt-mayusculas">Actividad Reciente</h3>
			</div>
		</div>
	</div>

	<div class="container-fluid">
        {% for control in controles%}
            {% include 'YoExistoContenidoBundle:Blocks:reporte.html.twig' with {'control' : control} %}
        {% endfor %}
	</div>

{% endblock %}