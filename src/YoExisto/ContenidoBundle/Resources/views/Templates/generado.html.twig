{% extends "YoExistoContenidoBundle:Templates:dashboard.html.twig" %}


{% block menu %}
	{% include 'YoExistoContenidoBundle:Blocks:menu-user.html.twig' %}
{% endblock %}

{% block contenidointerno %}
	<div class="container-fluid">
		<div class="row espacio-a fondo-naranjabajo">
			<div class="col-lg-11 col-md-11 col-sm-10 col-xs-10 text-center">
				<h3 class="txt-mayusculas">Generar Control</h3>
				<h4>
					Llene los datos a continuación para generar control
				</h4>
			</div>
			<div class="col-lg-1 col-md-1 col-sm-2 col-xs-2 text-right">
				<h3>
					<a href="{{ path('yoexisto_reciente') }}" class="link-yep icono-cerrar" alt=""></a>
				</h3>
			</div>
		</div>
		<div class="row espacio-a">
			<div class="col-lg-4 col-md-4 col-sm-4 col-lg-offset-4 col-md-offset-4 col-sm-offset-4 text-center">
				<h4 class="txt-mayusculas espacio-a">El control ha sido generado</h4>
			</div>
		</div>
		<div class="row espacio-a">
			<div class="col-lg-4 col-md-4 col-sm-4 col-lg-offset-4 col-md-offset-4 col-sm-offset-4 txt-mayusculas">
				{% image '@YoExistoContenidoBundle/Resources/public/images/generado.png' %}
				<img src="{{ asset_url }}" title="" alt="" class="img-responsive img-centrar img-icono-grande" />
				{% endimage %}
			</div>
		</div>
		<div class="row espacio-a">
			<div class="col-md-6 col-md-offset-3 txt-mayusculas">
				<div class="form-group">
					<button class="btn btn-home btn-tipo-a">Volver a la página principal</button>
				</div>
			</div>
		</div>
	</div>

{% endblock %}